{% extends "@FOSUser/layout.html.twig" %}
{% block fos_user_content %}

    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title text-center">My Messages</h3>
        </div>
        {% for message in messages %}
            <div class="panel-body">
                <p class="btn btn-info">From: {{ message.senderId }}</p>
                <p class="panel-info">Date: {{ message.date }}</p>
                <p class="panel-body">{{ message.content }}</p>
            </div>
        {% endfor %}
    </div>

{% endblock fos_user_content %}